{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsSchematicSchema",
  "title": "Schematic Options Schema",
  "type": "object",
  "properties": {
    "field": {
      "type": "string",
      "description": "Field",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Which field do you want to modify?"
    },
    "value": {
      "type": "string",
      "description": "Value",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "Which value do you want to assign?"
    }
  },
  "required": []
}
